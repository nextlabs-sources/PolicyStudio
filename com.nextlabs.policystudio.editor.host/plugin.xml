<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.nextlabs.policystudio.editor">
      <editor
            context="HOST"
            displayName="Computer Component"
            editorClass="com.nextlabs.policystudio.editor.host.DesktopComponentEditor"
            icon="icons/desktopComponent.gif"
                        panelClass="com.bluejungle.destiny.policymanager.editor.ComponentEditorPanel"
            type="COMPONENT">
      </editor>
   </extension>   
   <extension
         point="com.nextlabs.policystudio.tab">
